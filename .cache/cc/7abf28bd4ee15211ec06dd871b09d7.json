ion-count: infinite;\n   \tanimation-timing-function: linear; \n\tz-index: 1;\n}\n\n.grid-item-container:hover .slide{\n\t\n\tmargin-left: 1.3rem;\n\ttransition: 0.4s;\n}\n\n@keyframes spin {\n    from {\n        transform:rotate(0deg);\n    }\n    to {\n        transform:rotate(360deg);\n\t}\n}\n\n/*------------\nMODAL\n-------------*/\n\n\n.modal{/*background only*/\n\tbackground-color: rgba(0,0,0,0.6);\n    height: 100%;\n    left: 0;\n    position: fixed;\n    top: 0;\n    width: 100%;\n    z-index: 5000;\n}\n\n.modal-content{\n  \tanimation-duration: 0.3s;\n  \tanimation-name: modalopen;\n\tbackground: linear-gradient(120deg, #fdfbfb 0%, #ebedee 100%);\t\n\tbox-shadow: 5px 5px 15px rgba(37, 37, 37, 0.1),\t\n\t\t-5px -5px 15px rgba(0,0,0,0.1);\t\n\tborder: 1px solid rgb(223, 223, 223);\n\tmargin: 4% auto;\t\n  \theight: 80vh;\n  \toverflow: auto;\n  \twidth: 80vw;\n}\n\n@keyframes modalopen{\n  \tfrom{opacity: 0}\n  \tto{opacity: 1}\n}\n\n.modal-content--header{\t\t\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: space-between;\n\tpadding-right: 1rem;\n\tbackground: rgb(131,58,180);\n\tbackground: linear-gradient(90deg, rgba(131,58,180,1) 0%, rgba(253,29,29,1) 50%, rgba(255,149,0,1) 100%);\n\tcolor: white;\n}\n\n.model--header-image{\n\tjustify-self: flex-start;\n\twidth: auto;\n\theight: 100px;\n}\n\n.modal-content--header h2{\n\ttext-align: center;\n\tfont-size: 3rem;\n\ttext-shadow: 2px 3px 15px rgba(0,0,0,0.2), 0px -3px 35px rgba(255,255,255,0.2);\n\t\n}\n\n.modal-content--body{\n\tpadding: 2rem;\n}\n\n\n\n/*------------\nFOOTER\n-------------*/\nfooter{\n\talign-items: flex-start;\n\tbackground-color: black;\n\tcolor: white;\n\tdisplay: flex;\n\tflex-wrap: wrap;\n\tpadding:3rem;\n}\nfooter>*{\n\tmargin-right: 5rem;\n}\n\n.hidden{\n\tdisplay: none;\n}\n\n"},"lineCount":null}},"error":null,"hash":"f6d52916d42f572dc4cbe699e3078b03","cacheData":{"env":{}}}