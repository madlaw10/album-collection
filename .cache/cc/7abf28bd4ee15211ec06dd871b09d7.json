1px solid white;\n\tdisplay: grid;\n\tbox-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\n\tgrid-template-columns: 200px;\n\tgrid-template-rows: 200px;\n\tbackground-color: white;\n\tmargin:1.2rem;\n\theight: 200px;\n\twidth: 200px;\n}\n\n.grid-image{\n\tgrid-column-start: 1;\n\tgrid-column-end: -1;\n\tgrid-row-start: 1;\n\tgrid-row-end: -1; \n\theight: auto;\n\twidth: 200px;\n\tz-index: 2;\n}\n\n.item-name{\n\tbackground-color: rgba(0,0,0, 0.4);\n\tpadding: 0.3rem;\n\twidth: 100%;\n\tcolor: #FFF;\n\tmargin: auto 0;\n\tgrid-column-start: 1;\n\tgrid-column-end: -1;\n\tgrid-row-start: 1;\n\tgrid-row-end: -1;\n\ttext-shadow: 2px 2px 5px #000;\n\tz-index: 3;\n}\n\n.grid-item-container:hover{\n\tcursor: pointer;\n\ttransition: 0.2s;\n\tbox-shadow: 0 8px 16px 0 rgba(0,0,0,0.5);\n}\n\n.slide{\n\tanimation-name: spin;\n\tanimation-duration: 1000ms;\n\tanimation-iteration-count: infinite;\n   \tanimation-timing-function: linear; \n\tz-index: 1;\n}\n\n.grid-item-container:hover .slide{\n\t\n\tmargin-left: 1.3rem;\n\ttransition: 0.4s;\n}\n\n@keyframes spin {\n    from {\n        transform:rotate(0deg);\n    }\n    to {\n        transform:rotate(360deg);\n\t}\n}\n\n/*------------\nMODAL\n-------------*/\n\n\n.modal{/*background only*/\n\tbackground-color: rgba(0,0,0,0.6);\n    height: 100%;\n    left: 0;\n    position: fixed;\n    top: 0;\n    width: 100%;\n    z-index: 5000;\n}\n\n.modal-content{\n  \tanimation-duration: 0.3s;\n  \tanimation-name: modalopen;\n\tbackground: linear-gradient(120deg, #fdfbfb 0%, #ebedee 100%);\t\n\tbox-shadow: 5px 5px 15px rgba(37, 37, 37, 0.1),\t\n\t\t-5px -5px 15px rgba(0,0,0,0.1);\t\n\tborder: 1px solid rgb(223, 223, 223);\n\tmargin: 4% auto;\t\n  \theight: 80vh;\n  \toverflow: auto;\n  \twidth: 80vw;\n}\n\n@keyframes modalopen{\n  \tfrom{opacity: 0}\n  \tto{opacity: 1}\n}\n\n.modal-content--header{\t\t\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: space-between;\n\tpadding-right: 1rem;\n\tbackground: rgb(131,58,180);\n\tbackground: linear-gradient(90deg, rgba(131,58,180,1) 0%, rgba(253,29,29,1) 50%, rgba(255,149,0,1) 100%);\n\tcolor: white;\n}\n\n.model--header-image{\n\tjustify-self: flex-start;\n\twidth: auto;\n\theight: 100px;\n}\n\n.modal-content--header h2{\n\ttext-align: center;\n\tfont-size: 3rem;\n\ttext-shadow: 2px 3px 15px rgba(0,0,0,0.2), 0px -3px 35px rgba(255,255,255,0.2);\n\t\n}\n\n.modal-content--body{\n\tpadding: 2rem;\n}\n\n\n\n/*------------\nFOOTER\n-------------*/\nfooter{\n\talign-items: flex-start;\n\tbackground-color: black;\n\tcolor: white;\n\tdisplay: flex;\n\tflex-wrap: wrap;\n\tpadding:3rem;\n}\nfooter>*{\n\tmargin-right: 5rem;\n}\n\n/*------------\nINPUTS\n-------------*/\ninput{\n\tbackground-color: rgba(255, 255, 255, 0.3);\n\tpadding: 1rem;\n\tborder: 1px solid white;\n\n}\n\n\n\n\n\n\n.hidden{\n\tdisplay: none;\n}\n\n"},"lineCount":null}},"error":null,"hash":"e17cb793301af0b5bbc363acc78f82a4","cacheData":{"env":{}}}