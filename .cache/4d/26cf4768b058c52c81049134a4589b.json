: 1;\r\n\tgrid-row-end: -1; \r\n\theight: auto;\r\n\twidth: 200px;\r\n\tz-index: 2;\r\n}\r\n\r\n.item-name{\r\n\tbackground-color: rgba(0,0,0, 0.4);\r\n\tpadding: 0.3rem;\r\n\twidth: 100%;\r\n\tcolor: #FFF;\r\n\tmargin: auto 0;\r\n\tgrid-column-start: 1;\r\n\tgrid-column-end: -1;\r\n\tgrid-row-start: 1;\r\n\tgrid-row-end: -1;\r\n\ttext-shadow: 2px 2px 5px #000;\r\n\tz-index: 3;\r\n}\r\n\r\n.grid-item-container:hover{\r\n\tcursor: pointer;\r\n\ttransition: 0.2s;\r\n\tbox-shadow: 0 8px 16px 0 rgba(0,0,0,0.5);\r\n}\r\n\r\n.slide{\r\n\tanimation-name: spin;\r\n\tanimation-duration: 1000ms;\r\n\tanimation-iteration-count: infinite;\r\n   \tanimation-timing-function: linear; \r\n\tz-index: 1;\r\n}\r\n\r\n.grid-item-container:hover .slide{\r\n\t\r\n\tmargin-left: 1.3rem;\r\n\ttransition: 0.4s;\r\n}\r\n\r\n@keyframes spin {\r\n    from {\r\n        transform:rotate(0deg);\r\n    }\r\n    to {\r\n        transform:rotate(360deg);\r\n\t}\r\n}\r\n\r\n/*------------\r\nMODAL\r\n-------------*/\r\n\r\n\r\n.modal{/*background only*/\r\n\tbackground-color: rgba(0,0,0,0.6);\r\n    height: 100%;\r\n    left: 0;\r\n    position: fixed;\r\n    top: 0;\r\n    width: 100%;\r\n    z-index: 5000;\r\n}\r\n\r\n.modal-content{\r\n  \tanimation-duration: 0.3s;\r\n  \tanimation-name: modalopen;\r\n\tbackground: linear-gradient(120deg, #fdfbfb 0%, #ebedee 100%);\t\r\n\tbox-shadow: 5px 5px 15px rgba(37, 37, 37, 0.1),\t\r\n\t\t-5px -5px 15px rgba(0,0,0,0.1);\t\r\n\tborder: 1px solid rgb(223, 223, 223);\r\n\tmargin: 4% auto;\t\r\n  \theight: 80vh;\r\n  \toverflow: auto;\r\n  \twidth: 80vw;\r\n}\r\n\r\n@keyframes modalopen{\r\n  \tfrom{opacity: 0}\r\n  \tto{opacity: 1}\r\n}\r\n\r\n.modal-content--header{\r\n\tbox-shadow: 0px 0px 18px 2px rgba(0,0,0,0.24);\t\t\r\n\tdisplay: flex;\r\n\talign-items: center;\r\n\tjustify-content: space-between;\r\n\tpadding-right: 1rem;\r\n\tbackground: rgb(131,58,180);\r\n\tbackground: linear-gradient(90deg, rgba(131,58,180,1) 0%, rgba(253,29,29,1) 50%, rgba(255,149,0,1) 100%);\r\n\tcolor: white;\r\n}\r\n\r\n.model--header-image{\r\n\tjustify-self: flex-start;\r\n\twidth: auto;\r\n\theight: 100px;\r\n}\r\n\r\n.modal-content--header h2{\r\n\ttext-align: center;\r\n\tfont-size: 3rem;\r\n\ttext-shadow: 2px 3px 15px rgba(0,0,0,0.2), 0px -3px 35px rgba(255,255,255,0.2);\r\n\t\r\n}\r\n\r\n.modal-content--body{\r\n\tpadding: 2rem;\r\n}\r\n\r\n.hidden{\r\n\tdisplay: none;\r\n}"},"lineCount":null}},"error":null,"hash":"fbda4d5b170ba68741059cc4e988c5b6","cacheData":{"env":{}}}